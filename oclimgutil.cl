__constant float v5c[] = {
  - 4.667,-4.083, 0.000, 4.083, 4.667,
  -10.024,-0.963, 0.000, 0.963,10.024,
  -14.120, 3.622, 0.000,-3.622,14.120,
  -10.024,-0.963, 0.000, 0.963,10.024,
  - 4.667,-4.083, 0.000, 4.083, 4.667,
};

__kernel void edgevec_plab(global float2 *dst, global uint32_t *in, int iw, int ih) {
  const int x = get_global_id(0), y = get_global_id(1);
  if (x >= iw || y >= ih) return;
  const int p0 = y * iw + x;

  float3 vx3 = 0, vy3 = 0;

  for(int yy=-2;yy<=2;yy++) {
    for(int xx=-2;xx<=2;xx++) {
      float3 s = unpacklab(in[mirror(x+xx, y+yy,iw,ih)]);
      vx3 += v5c[(xx+2)+(yy+2)*5] * s;
      vy3 += v5c[(yy+2)+(xx+2)*5] * s;
    }
  }

  float3 ivlen3 = vx3*vx3 + vy3*vy3;

  float ivlen, vx, vy;

  if (ivlen3.s0 >= ivlen3.s1 && ivlen3.s0 >= ivlen3.s2) {
    ivlen = ivlen3.s0; vx = vx3.s0; vy = vy3.s0;
  } else if (ivlen3.s1 >= ivlen3.s2) {
    ivlen = ivlen3.s1; vx = vx3.s1; vy = vy3.s1;
  } else {
    ivlen = ivlen3.s2; vx = vx3.s2; vy = vy3.s2;
  }

  if (ivlen3.s0 >= 1e-6 && (vx3.s0 * vx + vy3.s0 * vy < 0)) {
    vx = -vx; vy = -vy;
  }

  if (ivlen > 1e-10) {
    ivlen = rsqrt(ivlen);
    vx *= ivlen; vy *= ivlen;
  } else {
    vx = vy = 0.70710678118f;
  }

  dst[p0] = (float2) (vx, vy);
}

__kernel void edgevec_f(global float2 *dst, global float *in, int iw, int ih) {
  const int x = get_global_id(0), y = get_global_id(1);
  if (x >= iw || y >= ih) return;
  const int p0 = y * iw + x;

  float vx = 0, vy = 0;

  for(int yy=-2;yy<=2;yy++) {
    for(int xx=-2;xx<=2;xx++) {
      float s = in[mirror(x+xx, y+yy,iw,ih)];
      vx += v5c[(xx+2)+(yy+2)*5] * s;
      vy += v5c[(yy+2)+(xx+2)*5] * s;
    }
  }

  float ivlen = vx * vx + vy * vy;

  if (ivlen > 1e-10) {
    ivlen = rsqrt(ivlen);
    vx *= ivlen; vy *= ivlen;
  } else {
    vx = vy = 0.70710678118f;
  }

  dst[p0] = (float2) (vx, vy);
}

__kernel void edge_plab(global float *out, global uint32_t *in, int iw, int ih) {
  const int x = get_global_id(0), y = get_global_id(1);
  if (x >= iw || y >= ih) return;

  float3 sum = 0, t;

  t = unpacklab(in[mirror(x, y-1,iw,ih)]) + unpacklab(in[mirror(x-1, y,iw,ih)]) - unpacklab(in[mirror(x, y+1,iw,ih)]) - unpacklab(in[mirror(x+1, y,iw,ih)]);
  sum += (unpacklab(in[mirror(x-1, y-1,iw,ih)]) - unpacklab(in[mirror(x+1, y+1,iw,ih)])) * t;

  t = unpacklab(in[mirror(x, y-1,iw,ih)]) - unpacklab(in[mirror(x-1, y,iw,ih)]) + unpacklab(in[mirror(x+1, y,iw,ih)]) - unpacklab(in[mirror(x, y+1,iw,ih)]);
  sum += (unpacklab(in[mirror(x+1, y-1,iw,ih)]) - unpacklab(in[mirror(x-1, y+1,iw,ih)])) * t;

  sum = max((float3) (0, 0, 0), sum);

  out[y*iw + x] = (sum.s0 + sum.s1 + sum.s2) > 0 ? sqrt(sum.s0 + sum.s1 + sum.s2) : 0.0f;
}

__kernel void edge_f_f(global float *out, global float *in, int iw, int ih) {
  const int x = get_global_id(0), y = get_global_id(1);
  if (x >= iw || y >= ih) return;

  float sum = 0, t;

  t = in[mirror(x, y-1,iw,ih)] + in[mirror(x-1, y,iw,ih)] - in[mirror(x, y+1,iw,ih)] - in[mirror(x+1, y,iw,ih)];
  sum += (in[mirror(x-1, y-1,iw,ih)] - in[mirror(x+1, y+1,iw,ih)]) * t;

  t = in[mirror(x, y-1,iw,ih)] - in[mirror(x-1, y,iw,ih)] + in[mirror(x+1, y,iw,ih)] - in[mirror(x, y+1,iw,ih)];
  sum += (in[mirror(x+1, y-1,iw,ih)] - in[mirror(x-1, y+1,iw,ih)]) * t;

  out[y*iw + x] = sqrt(max(0.0f, sum));
}

