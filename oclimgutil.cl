__kernel void pack_plab(global uint32_t *out, global float *in0, global float *in1, global float *in2, int iw, int ih) {
  const int x = get_global_id(0), y = get_global_id(1);
  if (x >= iw || y >= ih) return;
  const int p0 = y * iw + x;

  out[p0] = packlab((float3) (in0[p0], in1[p0], in2[p0]));
}

__kernel void unpack_plab(global float *out0, global float *out1, global float *out2, global uint32_t *in, int iw, int ih) {
  const int x = get_global_id(0), y = get_global_id(1);
  if (x >= iw || y >= ih) return;
  const int p0 = y * iw + x;

  float3 o = unpacklab(in[p0]);
  out0[p0] = o.x;
  out1[p0] = o.y;
  out2[p0] = o.z;
}

