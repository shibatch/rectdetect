uint32_t packlab(float3 clab) {
  uint32_t ret = 0;
  ret = clamp(convert_uint_rtn(clab.z * 1024), 0U, 1023U);
  ret = (ret << 10) | clamp(convert_uint_rtn(clab.y * 1024), 0U, 1023U);
  ret = (ret << 12) | clamp(convert_uint_rtn(clab.x * 4096), 0U, 4095U);
  return ret;
}

float3 unpacklab(uint32_t plab) {
  float3 f = convert_float3((int3) (plab, plab >> 12, plab >> 22) & (int3) (4095, 1023, 1023));
  return f * (float3) (1.0f/4096, 1.0f/1024, 1.0f/1024) + (float3)(0.5f/4096, 0.5f/1024, 0.5f/1024);
}

int mirror(int x, int y, int iw, int ih) {
  int2 xy = (int2)(x, y);
  xy = clamp(xy, -xy, (int2)(iw, ih) * 2 - 2 - xy);
  return xy.x + xy.y * iw;
}

int mirror1(int x, int iw) {
  return clamp(x, -x, iw * 2 - 2 - x);
}

int repeat1(int x, int iw) {
  x = x < 0 ? x+iw : x;
  x = x >= iw ? x-iw : x;
  return x;
}

int repeat(int x, int y, int iw, int ih) {
  x = x < 0 ? x+iw : x;
  x = x >= iw ? x-iw : x;
  y = y < 0 ? y+ih : y;
  y = y >= ih ? y-ih : y;
  return iw * y + x;
}

float bicubicSub(float p0, float p1, float p2, float p3, float x) {
  float u, v, w;
  v = p1 - p2;
  w = p3 - p0;
  u = v * 3.0f + w;
  u = u * x   + (-4.0f * v + (p0 - p1 - w));
  u = u * x   + (p2 - p0);
  u = u * x * 0.5f + p1;
  return u;
}

float3 bicubicSub3(float3 p0, float3 p1, float3 p2, float3 p3, float x) {
  float3 u, v, w;
  v = p1 - p2;
  w = p3 - p0;
  u = v * 3.0f + w;
  u = u * x    + (-4.0f * v + (p0 - p1 - w));
  u = u * x    + (p2 - p0);
  u = u * x * 0.5f + p1;
  return u;
}

float bicubic(global float *p, float x, float y, int iw, int ih) {
  const int ix = (int)x, iy = (int)y;

  return bicubicSub(bicubicSub(p[mirror(ix-1, iy-1,iw,ih)], p[mirror(ix  , iy-1,iw,ih)], p[mirror(ix+1, iy-1,iw,ih)], p[mirror(ix+2, iy-1,iw,ih)], x-ix),
		    bicubicSub(p[mirror(ix-1, iy  ,iw,ih)], p[mirror(ix  , iy  ,iw,ih)], p[mirror(ix+1, iy  ,iw,ih)], p[mirror(ix+2, iy  ,iw,ih)], x-ix),
		    bicubicSub(p[mirror(ix-1, iy+1,iw,ih)], p[mirror(ix  , iy+1,iw,ih)], p[mirror(ix+1, iy+1,iw,ih)], p[mirror(ix+2, iy+1,iw,ih)], x-ix),
		    bicubicSub(p[mirror(ix-1, iy+2,iw,ih)], p[mirror(ix  , iy+2,iw,ih)], p[mirror(ix+1, iy+2,iw,ih)], p[mirror(ix+2, iy+2,iw,ih)], x-ix), y-iy);
}

float3 bicubicp(global uint32_t *p, float x, float y, int iw, int ih) {
  const int ix = (int)x, iy = (int)y;
  float3 f;
  f = bicubicSub3(bicubicSub3(unpacklab(p[mirror(ix-1, iy-1,iw,ih)]), unpacklab(p[mirror(ix  , iy-1,iw,ih)]), unpacklab(p[mirror(ix+1, iy-1,iw,ih)]), unpacklab(p[mirror(ix+2, iy-1,iw,ih)]), x-ix),
		  bicubicSub3(unpacklab(p[mirror(ix-1, iy  ,iw,ih)]), unpacklab(p[mirror(ix  , iy  ,iw,ih)]), unpacklab(p[mirror(ix+1, iy  ,iw,ih)]), unpacklab(p[mirror(ix+2, iy  ,iw,ih)]), x-ix),
		  bicubicSub3(unpacklab(p[mirror(ix-1, iy+1,iw,ih)]), unpacklab(p[mirror(ix  , iy+1,iw,ih)]), unpacklab(p[mirror(ix+1, iy+1,iw,ih)]), unpacklab(p[mirror(ix+2, iy+1,iw,ih)]), x-ix),
		  bicubicSub3(unpacklab(p[mirror(ix-1, iy+2,iw,ih)]), unpacklab(p[mirror(ix  , iy+2,iw,ih)]), unpacklab(p[mirror(ix+1, iy+2,iw,ih)]), unpacklab(p[mirror(ix+2, iy+2,iw,ih)]), x-ix), y-iy);
  return f;
}

uint32_t srgb2plab(uchar3 u) {
  const float xn = 0.950456f, zn = 1.088754f;

  int ir = s2l[u.s2];
  int ig = s2l[u.s1];
  int ib = s2l[u.s0];

  int cx = (((ir * (int)(0.412453f * 16384 + 0.5f) + ig * (int)(0.357580f * 16384 + 0.5f) + ib * (int)(0.180423f * 16384 + 0.5f)+(1<<14)) >> 15) * (int)(32768 / xn + 0.5f) + (1<<10))>>11;
  int cy = (((ir * (int)(0.212671f * 16384 + 0.5f) + ig * (int)(0.715160f * 16384 + 0.5f) + ib * (int)(0.072169f * 16384 + 0.5f)        )      )                           + (1<<10))>>11;
  int cz = (((ir * (int)(0.019334f * 16384 + 0.5f) + ig * (int)(0.119193f * 16384 + 0.5f) + ib * (int)(0.950227f * 16384 + 0.5f)+(1<<14)) >> 15) * (int)(32768 / zn + 0.5f) + (1<<10))>>11;

  int cl = (((cfunc2[cy >> 8] * (256 - (cy & 255)) + cfunc2[(cy >> 8) + 1] * (cy & 255)) >> 12) + 1) >> 1;

  int fx = cfunc[cx >> 8] * (256 - (cx & 255)) + cfunc[(cx >> 8) + 1] * (cx & 255); // <<24
  int fy = cfunc[cy >> 8] * (256 - (cy & 255)) + cfunc[(cy >> 8) + 1] * (cy & 255);
  int fz = cfunc[cz >> 8] * (256 - (cz & 255)) + cfunc[(cz >> 8) + 1] * (cz & 255);

  int fxy = (fx - fy + (1 << 7)) >> 8;
  int fyz = (fy - fz + (1 << 7)) >> 8;

  int ca = ((fxy * 8031 + (134744072 + (1 << 17))) >> 18);
  int cb = ((fyz * 3213 + (134744072 + (1 << 17))) >> 18);

  uint32_t ret = 0;
  ret = clamp(convert_uint_rtn(cb), 0U, 1023U);
  ret = (ret << 10) | clamp(convert_uint_rtn(ca), 0U, 1023U);
  ret = (ret << 12) | clamp(convert_uint_rtn(cl), 0U, 4095U);
  return ret;
}

float3 srgb2lab(uchar3 u) { return unpacklab(srgb2plab(u)); }

float icfunc(float ft) {
  if (ft > 0.20689270648f) {
    return ft * ft * ft;
  } else {
    return (ft - 16.0f/116) * (1.0f / 7.787f);
  }
}

uchar3 lab2srgb(float3 lab) {
  const float xn = 0.950456f, zn = 1.088754f;

  lab *= 256;

  float cy;
  if (lab.x > 0.20689270648f) {
    cy = (lab.x + 16) * (1.0f / 116.0f);
    cy = cy * cy * cy;
  } else {
    cy = lab.x * (1.0f / 903.3f);
  }

  float fy = (cfunc[clamp(convert_int_rtn(cy * 1024), 0, 1023)] + 9039) * (1.0f / 65536.0f);

  float fz = fy - (lab.z - 128) * (1.0f / 200.0f);
  float fx = fy + (lab.y - 128) * (1.0f / 500.0f);

  float cx = icfunc(fx) * xn;
  float cz = icfunc(fz) * zn;

  float r = cx *  3.240479f + cy * -1.537150f + cz * -0.498535f;
  float g = cx * -0.969256f + cy *  1.875991f + cz *  0.041556f;
  float b = cx *  0.055648f + cy * -0.204043f + cz *  1.057311f;

  uchar3 srgb;

  srgb.s2 = l2s[clamp(convert_int_rtn(r * 1024), 0, 1023)];
  srgb.s1 = l2s[clamp(convert_int_rtn(g * 1024), 0, 1023)];
  srgb.s0 = l2s[clamp(convert_int_rtn(b * 1024), 0, 1023)];

  return srgb;
}

uchar3 plab2srgb(uint32_t plab) { return lab2srgb(unpacklab(plab)); }

ulong xrandom(ulong s) {
  int n;
  ulong t = s;
  n = (s >> 24) & 63; t = ((t << n) | (t >> (64 - n))); t ^= 0xf3dd0fb7820fde37UL;
  n = (s >>  6) & 63; t = ((t << n) | (t >> (64 - n))); t ^= 0xe6c6ac2c59e52811UL;
  n = (s >> 18) & 63; t = ((t << n) | (t >> (64 - n))); t ^= 0x2fc7871fff7c5b45UL;
  n = (s >> 48) & 63; t = ((t << n) | (t >> (64 - n))); t ^= 0x47c7e1f70aa4f7c5UL;
  n = (s >>  0) & 63; t = ((t << n) | (t >> (64 - n))); t ^= 0x094f02b7fb9ba895UL;
  n = (s >> 12) & 63; t = ((t << n) | (t >> (64 - n))); t ^= 0x89afda817e744570UL;
  n = (s >> 36) & 63; t = ((t << n) | (t >> (64 - n))); t ^= 0xc7277d052c7bf14bUL;
  return t;
}

